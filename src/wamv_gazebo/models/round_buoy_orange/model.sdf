                <geometry>
                    <mesh>
                        <uri>meshes/mb_round_buoy.dae</uri>
                    </mesh>
                </geometry>
                <material>
                    <ambient>0.9921 0.1882 0</ambient>
                    <diffuse>0.9921 0.1882 0</diffuse>
                    <specular>0.9921 0.1882 0 1</specular>
                    <pbr>
                        <metal>
