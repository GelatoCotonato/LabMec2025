            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <mass>3.5</mass>
                <inertia>
                    <ixx>0.0875</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0875</iyy>
                    <iyz>0</iyz>
                    <izz>0.0875</izz>
