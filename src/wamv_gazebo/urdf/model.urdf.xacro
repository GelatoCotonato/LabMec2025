              <bias_mean>0.04</bias_mean>
              <bias_stddev>0.006</bias_stddev>
              <dynamic_bias_stddev>0.0003</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>150.0</dynamic_bias_correlation_time>
              <precision>0.006</precision>
